<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="demo.yc.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".anim.FrameActivity">
        </activity>
        <activity android:name=".anim.TweenActivity">
        </activity>
        <activity android:name=".anim.PropertyActivity">
        </activity>
        <activity android:name=".interpolator.InterpolatorActivity">
        </activity>
        <activity android:name=".property.ObjectAnimatorActivity">
        </activity>
        <activity android:name=".property.ValueAnimatorActivity">
        </activity>
        <activity android:name=".property.AnimatorSetActivity">
        </activity>
        <activity android:name=".property.PathActivity">
        </activity>
        <activity android:name=".RippleActivity">
        </activity>
    </application>

</manifest>